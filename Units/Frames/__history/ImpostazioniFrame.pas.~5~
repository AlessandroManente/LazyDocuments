unit ImpostazioniFrame;

interface

uses
  Winapi.Windows,
  Winapi.Messages,
  System.SysUtils,
  System.Variants,
  System.Classes,
  Vcl.Graphics,
  Vcl.Controls,
  Vcl.Forms,
  Vcl.Dialogs;

type
  TImpostazioniFrm = class(TFrame)
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  ImpostazioniFrm: TImpostazioniFrm;

function GetCercaFrm: TImpostazioniFrm;

implementation

function GetCercaFrm: TImpostazioniFrm;
begin
  if not Assigned(ImpostazioniFrm) then
    ImpostazioniFrm := TImpostazioniFrm.Create(Application);
  Result := ImpostazioniFrm;
end;

{$R *.dfm}

end.
